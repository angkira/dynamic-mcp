{
  "name": "@dynamic-mcp/server",
  "version": "1.0.0",
  "description": "Backend for Dynamic MCP management",
  "main": "dist/app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:mcp-servers": "echo 'Using main build output in dist/mcp-servers'",
    "build:all": "npm run build",
    "start": "node dist/app.js",
    "dev": "nodemon",
    "dev:watch": "concurrently \"tsc -w\" \"nodemon --delay 1000ms dist/app.js\"",
    "test": "node --test test/**/*.test.js",
    "clean": "rm -rf dist",
    "prisma:generate": "prisma generate --schema=../prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=../prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=../prisma/schema.prisma",
    "seed": "node seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dynamic-mcp/shared": "file:../shared",
    "@fastify/autoload": "^6.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/type-provider-typebox": "^5.2.0",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@octokit/oauth-app": "^8.0.1",
    "@prisma/client": "^6.13.0",
    "@sinclair/typebox": "^0.34.38",
    "bcrypt": "^5.1.1",
    "dotenv": "^17.2.1",
    "fastify": "^5.4.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.1",
    "google-auth-library": "^10.2.1",
    "json-schema-to-ts": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "openai": "^5.11.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@dynamic-mcp/typescript-config": "file:../packages/typescript-config",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.14.13",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.13.0",
    "typescript": "^5.5.4"
  },
  "_moduleAliases": {
    "@shared": "../shared/dist",
    "@shared-prisma": "../shared/prisma-client"
  }
}
